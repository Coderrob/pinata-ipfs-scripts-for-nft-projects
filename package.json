  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{js,ts} --ignore-pattern 'src/bak/**'",
    "lint:fix": "eslint src/**/*.{js,ts} --ignore-pattern 'src/bak/**' --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{js,ts}\" --ignore-path .gitignore",
    "complexity": "eslint src/**/*.{js,ts} --ignore-pattern 'src/bak/**' --rule \"complexity: [error, 5]\"",
    "duplication": "jscpd src/ --ignore \"src/bak/**\"",
    "knip": "knip",
    "validate": "npm run lint && npm run format:check && npm run complexity && npm run duplication && npm run knip && npm run test"
  },
  "version": "2.0.0"
